        "source": [
            "# Modify values in the config and save a new version\n",
            "\n",
            "config[\"target_phrase\"] = [\"how do you wanna do this\"]\n",
            "config[\"model_name\"] = config[\"target_phrase\"][0].replace(\" \", \"_\")\n",
            "config[\"n_samples\"] = 70000\n",
